  //    vspd *= .5; 
  //}
  if state=0
  {
   if vspd &gt; 5 
   {
       vspd = 5; 
   }
  }
  repeat(abs(vspd)) 
  {
      if sign(vspd) &lt; 0
      {
          if !place_meeting(x,y-1,obj_block)
          {
              y += sign(vspd);
          }
          else
          {
              vspd = 0;
          }
      }
      else 
