 //
 shake=shakex
 gamepad_set_vibration(0,0,0)
 if (keyboard_check(key_dash) or pad_dash)
 {
  direction=dir
  speed=longdash
  state=1
 }
}

if state=1
{
 if shake&gt;0 {shake-=0.03}
 gamepad_set_vibration(0,shake,shake)
 blurr(80,15)
 grav=0
